# n8n Environment Configuration
# Copy this file to .env and fill in your values

# Domain Configuration
DOMAIN_NAME=freijstack.com
SUBDOMAIN_N8N=n8n
SSL_EMAIL=your-email@example.com

# Timezone
GENERIC_TIMEZONE=Europe/Paris

# n8n Configuration
# For production, set these to secure values
N8N_ENCRYPTION_KEY=your-secure-encryption-key-here
N8N_JWT_SECRET=your-secure-jwt-secret-here

# Admin credentials (for first login)
N8N_DEFAULT_USER_PASSWORD=your-secure-password

# Database (optional - defaults to SQLite)
# DB_TYPE=postgres
# DB_POSTGRE_HOST=postgres
# DB_POSTGRE_PORT=5432
# DB_POSTGRE_DB=n8n
# DB_POSTGRE_USER=n8n
# DB_POSTGRE_PASSWORD=secure-password

# Redis (optional - for distributed execution)
# QUEUE_TYPE=redis
# REDIS_HOST=redis
# REDIS_PORT=6379

# Webhook configuration
# WEBHOOK_TUNNEL_URL=https://n8n.freijstack.com/

# Security
N8N_SECURE_COOKIE=true
N8N_SECURE_COOKIE_NAME=n8n-session

# Feature flags
FEATURE_FLAGS_ENFORCE_SETTINGS_UI_FINE_GRAINED_PERMISSIONS=true
