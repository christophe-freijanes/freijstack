{
  "branches": ["master"],
  "tagFormat": "v${version}",
  "plugins": [
    ["@semantic-release/commit-analyzer", { "releaseRules": [{ "type": "refactor", "release": "patch" }] }],
    "@semantic-release/release-notes-generator",
    ["@semantic-release/changelog", { "changelogFile": "CHANGELOG.md" }],
    ["@semantic-release/github", { "repositoryUrl": "https://github.com/christophe-freijanes/freijstack.git" }]
  ]
}
